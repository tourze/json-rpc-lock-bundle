{
  "name": "tourze/json-rpc-lock-bundle",
  "description": "JsonRPC锁定处理",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "symfony/config": "^6.4",
    "symfony/cache-contracts": "^3",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/service-contracts": "^2.5 || ^3",
    "tourze/json-rpc-core": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\JsonRPCLockBundle\\": "src"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  }
}